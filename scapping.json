{
  "manifest_version": 2,
  "name": "site-blocker",
  "version": "1.0.0",
  "description": "Site blocker Chrome extension",
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*",
        "https://www.netflix.com/*"
      ],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": [
    "css/style.css"
  ]
}